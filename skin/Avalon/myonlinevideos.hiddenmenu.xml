<window>
  <controls>

	<control>
		<type>actiongroup</type>
		<description>action menu</description>
		<defaultcontrol>2</defaultcontrol>
		<onexit>50</onexit>
		<dimColor>00ffffff</dimColor>
		<buttonX>0</buttonX>
		<buttonY>0</buttonY>
		<buttonwidth>521</buttonwidth>
		<buttonheight>1080</buttonheight>
		
		<control>
			<type>image</type>
			<id>0</id>
			<width>1920</width>
			<height>1080</height>
			<texture>semi_trans_back_hidden_menu.png</texture>
			<visible>!Control.HasFocus(50)+control.isvisible(50)</visible>
			<animation effect="fade" time="450">visible</animation>
			<animation effect="fade" time="400">hidden</animation>
		</control>
	
	<control>
		<type>group</type>
		<animation effect="fade" time="250">WindowOpen</animation>
		<animation effect="fade" time="0">WindowClose</animation>
		<animation effect="slide" start="-150,0" end="0,0" tween="quadratic" easing="in" time="200" delay="0">visible</animation>
		<animation effect="fade" time="250">visible</animation>
		<animation effect="fade" time="0">hidden</animation>
		<control>
			<type>image</type>
			<id>0</id>
			<posY>15</posY>
			<posX>0</posX>
			<width>631</width>
			<height>1044</height>
			<texture>menu_bg.png</texture>
			<visible>!Control.HasFocus(50)+control.isvisible(50)</visible>
		</control>
	
		<control>
			<description>Heading text label</description>
			<type>label</type>
			<id>1</id>
			<posX>136</posX>
			<posY>128</posY>
			<label>924</label>
			<font>font22</font>
			<textcolor>ffffff</textcolor>
		</control>	
      <control>
        <description>button group</description>
        <type>group</type>        
		<animation effect="fade" time="250">WindowOpen</animation>
		<animation effect="fade" time="0">WindowClose</animation>
		<animation effect="slide" start="-150,0" end="0,0" tween="quadratic" easing="in" time="200" delay="0">visible</animation>
		<animation effect="fade" time="250">visible</animation>
		<animation effect="fade" time="0">hidden</animation>
		<posX>69</posX>
		<posY>215</posY>
        <layout>StackLayout(0, Vertical, true)</layout>
        <visible>!Control.HasFocus(50)</visible>
        <control>
			<description>View-As</description>
			<type>button</type>
			<id>2</id>
			<label>#OnlineVideos.Translation.LayoutList.Label</label>          
			<onleft>50</onleft>
			<onright>50</onright>
			<onup>13</onup>
			<textureFocus>hiddenmenu_item_selected.png</textureFocus>
			<textureNoFocus>-</textureNoFocus>
			<width>459</width>
			<height>70</height>
			<textXOff>66</textXOff>
			<textYOff>17</textYOff>
        </control>
        <control>
			<type>button</type>
			<description>Search</description>
			<id>10</id>
			<onleft>50</onleft>
			<onright>50</onright>
