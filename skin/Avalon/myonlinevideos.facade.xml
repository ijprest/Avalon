<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
		<animation effect="fade" start="0" end="100" time="300" delay="350">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="0" delay="0">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
			<control>
				<description>listcontrol</description>
				<type>listcontrol</type>
				<id>50</id>
				<onleft>2</onleft>
				<onright>80</onright>
				<scrollOffset>1</scrollOffset>
				<posX>678</posX>
				<posY>236</posY>
				<width>1180</width>
				<height>700</height>
				<textXOff>50</textXOff>
				<textXOff2>1155</textXOff2>
				<textureHeight>55</textureHeight>
				<spinCanFocus>no</spinCanFocus>
				<textureFocus>listcontrol_item_selected_large.png</textureFocus>
				<dimColor>ffffffff</dimColor>
			</control>
			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>100</posX>
				<posY>242</posY>
				<width>1650</width>
				<height>600</height>
				<onleft>2</onleft>
				<onright>50</onright>
          
			  <imageFolder>-</imageFolder>          
			  <imageFolderFocus>-</imageFolderFocus>  
			  <showFrame>true</showFrame>          
			  <dimColor>90ffffff</dimColor>
          
			<enableFocusZoom>no</enableFocusZoom>                    
			<zoomXPixels>40</zoomXPixels>
			<zoomYPixels>30</zoomYPixels>          
			<hideUnfocusTexture>no</hideUnfocusTexture>          
			<renderFocusText>no</renderFocusText>
			<renderUnfocusText>no</renderUnfocusText>                    
			<thumbZoom>yes</thumbZoom>
			<frameNoFocus>onlinevideos_thumb_nofocus.png</frameNoFocus>
			<frameFocus>onlinevideos_thumb_focus.png</frameFocus>
			<textureMask>onlinevideos_thumb_mask.png</textureMask>
          
          <!-- smallicons -->
			<itemWidth>228</itemWidth>
			<itemHeight>127</itemHeight>
			<textureWidth>228</textureWidth>
			<textureHeight>127</textureHeight>
			<thumbWidth>228</thumbWidth>
			<thumbHeight>127</thumbHeight>
			<thumbPosX>0</thumbPosX>
			<thumbPosY>0</thumbPosY>
          
          <!-- largeicons -->
			<itemWidthBig>275</itemWidthBig>
			<itemHeightBig>153</itemHeightBig>
			<textureWidthBig>275</textureWidthBig>
			<textureHeightBig>153</textureHeightBig>
			<thumbWidthBig>275</thumbWidthBig>
			<thumbHeightBig>153</thumbHeightBig>
			<thumbPosXBig>0</thumbPosXBig>
			<thumbPosYBig>0</thumbPosYBig>       
        </control>        
      </control>
    </control>
  </controls>
</window>