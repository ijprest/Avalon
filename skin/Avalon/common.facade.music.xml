				<textureDown>page_down_nofocus.png</textureDown>
				<textureUpFocus>page_up_focus.png</textureUpFocus>
				<textureDownFocus>page_down_focus.png</textureDownFocus>
				<scrollOffset>1</scrollOffset>
				<onleft>2</onleft>
				<onright>50</onright>
				<itemWidth>132</itemWidth>
				<itemHeight>134</itemHeight>
				<textureWidth>134</textureWidth>
				<textureHeight>134</textureHeight>
				<thumbWidth>103</thumbWidth>
				<thumbHeight>103</thumbHeight>
				<thumbPosX>14</thumbPosX>
				<thumbPosY>16</thumbPosY>
				<imageFolder>-</imageFolder>
