				<posY>236</posY>
				<height>650</height>
				<spinPosX>682</spinPosX>
				<spinPosY>1001</spinPosY>
				<spinAlign>left</spinAlign>
				<textXOff>78</textXOff>
				<dimColor>ffffffff</dimColor>
			</control>

            <control>
                <description>album view listcontrol</description>
                <type>listcontrol</type>
                <subtype>album</subtype>
                <id>50</id>
				<posX>594</posX>
				<posY>244</posY>
				<height>710</height>
				<textXOff>246</textXOff>
				<textYOff>30</textYOff>
